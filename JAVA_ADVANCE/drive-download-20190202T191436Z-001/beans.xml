<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config />

   
   <bean id="emp" class="spring.Emp">
   </bean>

   
   <bean id="salary1" class="spring.Salary">
<property name="basic" value="5000" />
      <property name="ta" value="4000" />
       <property name="da" value="3000" />	
   </bean>
<bean id="salary2" class="spring.Salary">
<property name="basic" value="50" />
      <property name="ta" value="40" />
       <property name="da" value="30" />	
   </bean>

</beans>